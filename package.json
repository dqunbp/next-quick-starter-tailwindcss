{
  "name": "nextjs-quick-starter-tailwindcss",
  "version": "0.1.0",
  "private": true,
  "author": "dqunbp",
  "keywords": [
    "nextjs",
    "starter",
    "template",
    "react",
    "ssr",
    "ssg",
    "tailwindcss",
    "react-query",
    "lingui.js",
    "i18n",
    "eslint",
    "husky",
    "prettier",
    "typescript",
    "axios"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:watchi18n": "concurrently \"npm run watch\"  \"next dev\"",
    "build": "npm run compile && next build",
    "start": "next start",
    "prettier": "prettier --config .prettierrc 'src/**/*.{ts,tsx,js,jsx}' --write",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "lingui compile",
    "watch": "npm-watch compile",
    "lint:ts": "tsc --noEmit -p tsconfig.json",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "watch": {
    "compile": {
      "patterns": [
        "src"
      ],
      "extensions": "po"
    }
  },
  "dependencies": {
    "@lingui/react": "^3.7.1",
    "axios": "^0.24.0",
    "cookie": "^0.4.1",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.12.1",
    "use-cookie-state": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@lingui/cli": "^3.7.1",
    "@lingui/loader": "^3.7.1",
    "@lingui/macro": "^3.7.1",
    "@types/cookie": "^0.4.0",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-macros": "^3.1.0",
    "concurrently": "^6.0.0",
    "eslint": "^8.1.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "npm-watch": "^0.11.0",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write"
  }
}
